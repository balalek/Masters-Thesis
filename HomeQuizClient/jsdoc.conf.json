{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(x)?$",
    "excludePattern": "(node_modules/|docs|build)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false,
      "useLongnameInNav": false
    }
  },
  "opts": {
    "destination": "./docs/jsdoc",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/docdash",
    "sort": true
  },
  "docdash": {
    "static": false,
    "sort": true,
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": [],
    "sectionOrder": [
      "Modules",
      "Classes",
      "Namespaces",
      "Externals",
      "Events",
      "Mixins",
      "Interfaces",
      "Global"
    ],
    "meta": {
      "title": "Home Quiz Documentation",
      "description": "Documentation for the Home Quiz application",
      "keyword": "react, quiz, documentation"
    },
    "outputSourceFiles": false,
    "navLevel": 2
  }
}